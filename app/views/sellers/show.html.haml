.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), sellers_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_seller_path(@seller), :class => 'btn btn-primary'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), seller_path(@seller), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
